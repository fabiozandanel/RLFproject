  doi = {10.1111/j.1365-2966.2009.15655.x},
  eprint = {0905.2418},
  keywords = {galaxies: clusters: individual: Coma, intergalactic medium},
  owner = {ensslin},
  primaryclass = {astro-ph.CO},
  timestamp = {2010.08.04}
}

